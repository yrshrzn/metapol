<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Due Diligence Form</title>
  </head>
  <body>
    <h1>Due Diligence Form</h1>
    <form action="/submit" method="POST">
      <div>
        <label for="foundingTeam">Founding Team:</label>
        <input type="number" name="foundingTeam" min="1" max="10" required>
      </div>
      <div>
        <label for="businessModel">Business Model:</label>
        <input type="number" name="businessModel" min="1" max="10" required>
      </div>
      <div>
        <label for="technology">Technology:</label>
        <input type="number" name="technology" min="1" max="10" required>
      </div>
      <div>
        <label for="market">Market:</label>
        <input type="number" name="market" min="1" max="10" required>
      </div>
      <div>
        <label for="tokenEconomics">Token Economics:</label>
        <input type="number" name="tokenEconomics" min="1" max="10" required>
      </div>
      <div>
        <label for="regulatoryCompliance">Regulatory Compliance:</label>
        <input type="number" name="regulatoryCompliance" min="1" max="10" required>
      </div>
      <div>
        <label for="community">Community:</label>
        <input type="number" name="community" min="1" max="10" required>
      </div>
      <div>
        <label for="funding">Funding:</label>
        <input type="number" name="funding" min="1" max="10" required>
      </div>
      <div>
        <label for="foundingTeamComment">Founding Team Comment:</label>
        <textarea name="foundingTeamComment"></textarea>
      </div>
      <div>
        <label for="businessModelComment">Business Model Comment:</label>
        <textarea name="businessModelComment"></textarea>
      </div>
      <div>
        <label for="technologyComment">Technology Comment:</label>
        <textarea name="technologyComment"></textarea>
      </div>
      <div>
        <label for="marketComment">Market Comment:</label>
        <textarea name="marketComment"></textarea>
      </div>
      <div>
        <label for="tokenEconomicsComment">Token Economics Comment:</label>
        <textarea name="tokenEconomicsComment"></textarea>
      </div>
      <div>
        <label for="regulatoryComplianceComment">Regulatory Compliance Comment:</label>
        <textarea name="regulatoryComplianceComment"></textarea>
      </div>
      <div>
        <label for="communityComment">Community Comment:</label>
        <textarea name="communityComment"></textarea>
      </div>
      <div>
        <label for="fundingComment">Funding Comment:</label>
        <textarea name="fundingComment"></textarea>
      </div>
      <div>
        <button type="submit">Submit</button>
      </div>
    </form>
  </body>
</html>
